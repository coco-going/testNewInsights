{
  "Azure": {
    "SubscriptionId": "{{AZURE_SUBSCRIPTION_ID}}",
    "TenantId": "{{AZURE_TENANT_ID}}",
    "ClientId": "{{AZURE_CLIENT_ID}}",
    "ClientSecret": "{{AZURE_CLIENT_SECRET}}",
    "KeyVaultUri": "https://mi-prod-kv-{{UNIQUE_SUFFIX}}.vault.azure.net/",
    "ResourceGroupName": "mi-prod-rg"
  },
  "OpenAI": {
    "Endpoint": "https://openai-prod-mi.openai.azure.com/",
    "ApiKey": "{{OPENAI_API_KEY}}",
    "DeploymentName": "gpt-4",
    "MaxTokens": 4000,
    "Temperature": 0.3,
    "MaxRetries": 3
  },
  "Database": {
    "ConnectionString": "{{SQL_CONNECTION_STRING}}",
    "DatabaseName": "mi-prod-db",
    "CommandTimeout": 30,
    "EnableRetryOnFailure": true
  },
  "Graph": {
    "TenantId": "{{AZURE_TENANT_ID}}",
    "ClientId": "{{GRAPH_CLIENT_ID}}",
    "ClientSecret": "{{GRAPH_CLIENT_SECRET}}",
    "Scopes": ["https://graph.microsoft.com/.default"],
    "Authority": "https://login.microsoftonline.com/"
  },
  "Agent": {
    "ManifestPath": "./manifest/manifest.json",
    "Name": "MarketingInsights",
    "Description": "AI-powered marketing insights agent",
    "Version": "1.0.0",
    "EnableTeamsIntegration": true,
    "EnableCopilotIntegration": true
  },
  "Search": {
    "ServiceName": "mi-prod-search-{{UNIQUE_SUFFIX}}",
    "ApiKey": "{{SEARCH_API_KEY}}",
    "IndexName": "transcripts",
    "Enabled": true
  },
  "Fabric": {
    "WorkspaceId": "{{FABRIC_WORKSPACE_ID}}",
    "DatasetId": "{{FABRIC_DATASET_ID}}",
    "ClientId": "{{FABRIC_CLIENT_ID}}",
    "ClientSecret": "{{FABRIC_CLIENT_SECRET}}",
    "Enabled": false
  }
}